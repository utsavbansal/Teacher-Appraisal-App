<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="utsavbansal.teacher_evaluation_app.admin_profile">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:textSize="40sp"
            android:layout_marginLeft="20dp"
            android:text="Name:"
            android:fontFamily="cursive"
            android:id="@+id/name_profile"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:textSize="40sp"
            android:layout_marginLeft="20dp"
            android:text="Id:"
            android:fontFamily="cursive"
            android:layout_below="@id/name_profile"
            android:id="@+id/email_id_profile"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:textSize="40sp"
            android:layout_marginLeft="20dp"
            android:text="Contact:"
            android:fontFamily="cursive"
            android:layout_below="@id/email_id_profile"
            android:id="@+id/contact_profile"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:textSize="40sp"
            android:layout_marginLeft="20dp"
            android:text="Password:"
            android:fontFamily="cursive"
            android:layout_below="@id/contact_profile"
            android:id="@+id/pwd_profile"
            />
        <Button
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_below="@id/pwd_profile"
            android:layout_centerInParent="true"
            android:background="@drawable/teacher_logout"
            android:id="@+id/admin_logout"
            />

    </RelativeLayout>
</FrameLayout>
